{
  "name": "aluvia",
  "version": "1.0.0",
  "description": "Official Aluvia proxy management SDK for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "tsc --watch",
    "dev": "npm run build:watch",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm run test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run test"
  },
  "keywords": [
    "proxy",
    "aluvia",
    "http",
    "sdk",
    "api",
    "typescript",
    "nodejs"
  ],
  "author": "Aluvia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtrella/aluvia-js.git"
  },
  "bugs": {
    "url": "https://github.com/xtrella/aluvia-js/issues"
  },
  "homepage": "https://github.com/xtrella/aluvia-js#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.4",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  }
}